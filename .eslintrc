{
    "env": {
        "browser": true,
        "jquery": true,
        "node": true,
        "es6": true
    },

    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },

    "rules": {
        "brace-style": [2, "1tbs"],
        "camelcase": 0,
        "comma-dangle": [2, "never"],
        "comma-spacing": 0,
        "comma-style": [2, "last"],
        "consistent-return": 1,
        "eol-last": 1,
        "indent": [2, 2, { "VariableDeclarator": { "var": 2 } }],
        "init-declarations": 1,
        "key-spacing": 0,
        "new-cap": [2, {"capIsNewExceptions": ["Deferred"]}],
        "no-alert": 0,
        "no-console": 0,
        "no-extra-parens": 0,
        "no-fallthrough": 0,
        "no-implicit-coercion": 1,
        "no-redeclare": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"],
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "space-after-keywords": 1,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "never"],
        "spaced-comment": 0,
        "use-isnan": 1,
        "yoda": [2, "never", { "exceptRange": true }]
    }
}
