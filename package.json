{
  "name": "livejournal-reader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "build": "browserify public/js/index.js -t babelify --outfile public/build/app.js; grunt",
    "watch": "watchify -t babelify --outfile public/build/app.js -v -d public/js/",
    "build-production": "browserify public/js/index.js -t babelify | uglifyjs -c unused,dead_code,warnings > public/build/app.js; grunt"
  },
  "dependencies": {
    "analytics-node": "^0.6.0",
    "async": "~0.2.10",
    "autolinker": "^0.17.1",
    "babel": "^5.4.3",
    "browserify-middleware": "^6.0.0",
    "classnames": "^2.1.1",
    "express": "3.2.0",
    "grunt-csso": "~0.5.0",
    "grunt-image-embed": "~0.3.1",
    "jquery": "^2.1.4",
    "livejournal": "^0.0.8",
    "lru-cache": "~2.5.0",
    "marked": "^0.3.3",
    "nprogress": "^0.2.0",
    "oauth": "~0.9.11",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.0",
    "react-document-title": "^1.0.2",
    "react-router": "1.0.0-beta2",
    "react-textarea-autosize": "^2.2.3",
    "request": "~2.33.0",
    "superagent": "^1.2.0",
    "underscore": "~1.6.0",
    "xmlrpc": "~1.2.0"
  },
  "devDependencies": {
    "optimist": "~0.6.1",
    "babelify": "^6.1.1",
    "watchify": "^3.2.1"
  }
}
