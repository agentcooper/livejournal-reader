<ol class="b-feed">
  <% _.each(journal.events, function(entry) { %>
  <li class="b-entry">
    <span class="b-boop__header"><a href="{{ entry.url }}">{{ entry.subject || '(no subject)' }}</a></span>

    <div class="b-post_aside">
      <img class="b-post__userpic" bo-src="entry.poster_userpic_url">
      <span class="b-post__username" bo-html="entry.username"></span>
    </div>

    <div class="b-entry__body">{{ entry.body }}</div>

    <p class="b-entry__tags">{{ entry.props.tags }}</p>

    <span class="b-boop__comments">{{ entry.reply_count }}</span>
  </li>
  <% }); %>
</ol>
